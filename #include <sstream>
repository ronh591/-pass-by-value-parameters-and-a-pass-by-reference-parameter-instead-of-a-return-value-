#include <iostream>
#include <string>
#include <sstream>
using namespace std;
int main()
{
string a;
double b;
cout << "enter string with numbers: " << endl;
getline(cin, a);
if (istringstream(a) >> b)
  cout << "YES:  " << b << "  ca commence avec des numeros" <<endl;
else
  cout << "NO  " <<a <<"  ca commence avec des lettres"  << endl;
}
